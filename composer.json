{
    "name": "magento/project-enterprise-edition",
    "description": "eCommerce Platform for Growth (Enterprise Edition)",
    "type": "project",
    "version": "2.4.3-p3",
    "license": [
        "proprietary"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/magento2-frontools.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/SomethingDigital-Magento2-Theme-BryantPark.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/composer-patcher.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/Magento2_SomethingDigital_Migration.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/Magento2_SomethingDigital_AdminNotify.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/Magento2_SomethingDigital_CmsMetaImage.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sdinteractive/Magento2_SomethingDigital_InvalidateAdminPasswords.git"
        }
    ],
    "require": {
        "php": "~7.3.0||~7.4.0",
        "composer/composer": "^1.10.21 || ^2.0.12",
        "dan0sz/resource-hints-magento2": "^1.2.0",
        "ethanyehuda/magento2-cronjobmanager": "^1.6",
        "jamescowie/composer-patcher": "1.0.3",
        "magento/magento-cloud-metapackage": "2.4.3",
        "magento/product-enterprise-edition": "2.4.3-p3",
        "msp/adminrestriction": "^1.3",
        "newrelic/monolog-enricher": "^1.0",
        "pelago/emogrifier": "~2.0.0||>=2.1.2",
        "snowdog/frontools": "dev-master",
        "somethingdigital/invalidate-admin-passwords": "dev-master",
        "somethingdigital/magento2-migration": "2.2.3",
        "somethingdigital/magento2-theme-bryantpark": "dev-integration/magento-v2.4.3-p1",
        "somethingdigital/module-admin-notify": "1.0.6",
        "somethingdigital/module-cms-meta-image": "1.0.6"
    },
    "require-dev": {
        "allure-framework/allure-phpunit": "~1.2.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "friendsofphp/php-cs-fixer": "~2.16.2",
        "lusitanian/oauth": "~0.8.10",
        "magento-ecg/coding-standard": "^3.1",
        "magento/magento-coding-standard": "^6.0",
        "pdepend/pdepend": "2.5.2",
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "~6.5.0",
        "sebastian/phpcpd": "~3.0.0",
        "squizlabs/php_codesniffer": "~3.5.4"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Magento\\Setup\\": "setup/src/Magento/Setup/",
            "Magento\\": "app/code/Magento/",
            "Zend\\Mvc\\Controller\\": "setup/src/Zend/Mvc/Controller/"
        },
        "psr-0": {
            "": [
                "app/code/",
                "generated/code/"
            ]
        },
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ],
        "exclude-from-classmap": [
            "**/dev/**",
            "**/update/**",
            "**/Test/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/",
            "Magento\\ToolkitFramework\\": "dev/tools/performance-toolkit/framework/Magento/ToolkitFramework/"
        }
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "scripts": {
        "generate-schema": [
            "php ./vendor/bin/ece-tools schema:generate || true"
        ],
        "install-coding-standard": [
            "([ $COMPOSER_DEV_MODE -eq 0 ] || vendor/bin/phpcs --config-set installed_paths ../../magento/magento-coding-standard/)"
        ],
        "post-install-cmd": [
            "Inviqa\\Command::patch",
            "@generate-schema"
        ],
        "post-update-cmd": [
            "Inviqa\\Command::patch",
            "@generate-schema"
        ]
    },
    "extra": {
        "magento-force": "override",
        "patches": {}
    }
}
